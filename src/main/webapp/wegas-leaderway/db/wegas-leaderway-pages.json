[{
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout",
        "children": [{
                "label": "indicateurs",
                "type": "List",
                "cssClass": "indicators",
                "direction": "horizontal",
                "children": [{
                        "type": "VariableDisplay",
                        "label": "Semaine N° : ",
                        "cssClass":"indicators-fraction",
                        "view": "fraction",
                        "variable": "week",
                        "dataSource": "VariableDescriptor"
                    },{
                        "type": "Button",
                        "label": ">",
                        "tooltips":"Semaine suivante",
                        "plugins": [{
                                "fn": "ExecuteScriptAction",
                                "cfg": {
                                    "onClick": "importPackage(com.wegas.core.script);\nthis.finishCurrentWeek()\n"
                                }
                            }]
                    },{
                        "type": "VariableDisplay",
                        "label": "Budget : ",
                        "cssClass":"indicators-text",
                        "view": "text",
                        "variable": "budget",
                        "dataSource": "VariableDescriptor"
                    },{
                        "type": "VariableDisplay",
                        "cssClass":"indicators-gauge",
                        "label": "Motivation générale",
                        "variable": "teamMotivation",
                        "view": "box",
                        "dataSource": "VariableDescriptor"
                    },
                    {
                        "type": "VariableDisplay",
                        "cssClass":"indicators-gauge",
                        "label": "Satisfaction client",
                        "variable": "clientsSatisfaction",
                        "view": "box",
                        "dataSource": "VariableDescriptor"
                    },
                    {
                        "type": "VariableDisplay",
                        "cssClass":"indicators-block",
                        "label": "Temps restant",
                        "variable": "actions",
                        "view": "box",
                        "dataSource": "VariableDescriptor"
                    }]
            },
            {
                "label": "Menu",
                "type": "List",
                "cssClass": "menu",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Button",
                        "label": "Dossiers",
                        "cssClass": "menu-folder",
                        "tooltips":"Dossiers des membres",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    }, {
                        "type": "Button",
                        "label": "Mandats",
                        "cssClass": "menu-tasklist",
                        "tooltips":"Liste des mandats",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "Button",
                        "label": "E-mail",
                        "cssClass": "menu-email",
                        "tooltips":"Réception e-mail",
                        "plugins": [{
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": "inbox"
                                }
                            },{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "Button",
                        "label": "Score",
                        "cssClass": "menu-score",
                        "tooltips":"Score des entreprises (temporaire)",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]

                    },
                    {
                        "type": "Button",
                        "label": "Dialogue",
                        "cssClass": "menu-tasklist",
                        "tooltips":"dialogue(temporaire)",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 7,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "LoginButton",
                        "label":"Login button",
                        "cssClass": "menu-loginbutton"
                    }]
            },
            {
                "type": "PageLoader",
                "id": "maindisplayarea",
                "cssClass": "body",
                "pageId": 2
            }]
    },
    {
        "id":2,
        "label":"Folder",
        "type":"Folder",
        "cssClass":"leaderway-folder"
    },
    {
        "id":4,
        "label":"TaskList",
        "type":"TaskList",
        "cssClass":"leaderway-tasklist"
    },
    {
        "id":5,
        "label":"Inbox",
        "type":"InboxDisplay",
        "variable":"inbox",
        "cssClass":"leaderway-inbox"
    },
    {
        "id":6,
        "label":"Score",
        "type":"Score",
        "title":"Top 5 des meilleurs entreprises actuelles",
        "cssClass":"leaderway-score"
    },
    {
        "id":7,
        "label":"Dialogue",
        "type":"Dialogue",
        "cssClass":"leaderway-dialogue",
        "toHide":".menu"
    }
]
