{"1":{
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout",
        "children": [{
                "label": "indicateurs",
                "type": "List",
                "cssClass": "leaderway-indicators",
                "direction": "horizontal",
                "children": [{
                        "type": "VariableDisplay",
                        "label": "Semaine N° : ",
                        "cssClass":"indicators-fraction",
                        "view": "fraction",
                        "variable": {
                            "name": "week"
                        }
                    },{
                        "type": "Button",
                        "label": ">",
                        "tooltip":"Semaine suivante",
                        "plugins": [{
                                "fn": "ExecuteScriptAction",
                                "cfg": {
                                    "onClick": "importPackage(com.wegas.core.script);\nthis.finishCurrentWeek()\n"
                                }
                            }]
                    },{
                        "type": "VariableDisplay",
                        "label": "Budget : ",
                        "cssClass":"indicators-text",
                        "view": "text",
                        "variable": {
                            "name": "budget"
                        }
                    },{
                        "type": "VariableDisplay",
                        "cssClass":"indicators-gauge",
                        "label": "Motivation générale",
                        "variable": {
                            "name": "teamMotivation"
                        },
                        "view": "box"
                    },
                    {
                        "type": "VariableDisplay",
                        "cssClass":"indicators-gauge",
                        "label": "Satisfaction client",
                        "variable": {
                            "name": "clientsSatisfaction"
                        },
                        "view": "box"
                    },
                    {
                        "type": "VariableDisplay",
                        "cssClass":"indicators-block",
                        "label": "Action restante",
                        "variable": {
                            "name": "actions"
                        },
                        "view": "box"
                    }]
            },
            {
                "label": "Menu",
                "type": "List",
                "cssClass": "leaderway-menu",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Button",
                        "cssClass": "menu-folder",
                        "tooltip":"Dossiers des membres",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    }, {
                        "type": "Button",
                        "cssClass": "menu-tasklist",
                        "tooltip":"Liste des mandats",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "Button",
                        "cssClass": "menu-email",
                        "tooltip":"Réception e-mail",
                        "plugins": [{
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": {
                                        "name":"inbox"
                                    }
                                }
                            },{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "Button",
                        "cssClass": "menu-score",
                        "tooltip":"Score des entreprises",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },
                    {
                        "type": "LoginButton",
                        "cssClass": "menu-loginbutton",
                        "tooltip": "Logout"
                    }]
            },
            {
                "type": "Text",
                "cssClass": "leaderway-feedback",
                "content": "<p></p>"
            },
            {
                "type": "PageLoader",
                "pageLoaderId": "maindisplayarea",
                "cssClass": "body",
                "defaultPageId": 2
            }]
    },
    "2":{
        "id":2,
        "label":"Folder",
        "type":"Folder",
        "cssClass":"leaderway-folder",
        "folderPageId": "2",
        "taskListPageId": "4",
        "dialoguePageId": "7",
        "targetPageLoaderId": "maindisplayarea"
    },
   "4": {
        "id":4,
        "label":"TaskList",
        "type":"TaskList",
        "cssClass":"leaderway-tasklist",
        "pickingMode":"true",
        "targetPageLoaderId": "maindisplayarea",
        "dialoguePageId": "7"
    },
    "5":{
        "id":5,
        "label":"Inbox",
        "type":"InboxDisplay",
        "variable": {
            "name": "inbox"
        },
        "cssClass":"leaderway-inbox",
        "dialoguePageId": "7",
        "targetPageLoaderId": "maindisplayarea"
    },
    "6":{
        "id":6,
        "label":"Score",
        "type":"Score",
        "title":"Top 5 des meilleurs entreprises actuelles",
        "cssClass":"leaderway-score",
        "maxRows":5,
        "dialoguePageId": "7",
        "targetPageLoaderId": "maindisplayarea"
    },
    "7":{
        "id":7,
        "label":"Dialogue",
        "type":"Dialogue",
        "cssClass":"leaderway-dialogue",
        "toHide":".leaderway-menu",
        "taskListPageId":"6",
        "dialoguePageId": "7",
        "targetPageLoaderId": "maindisplayarea"
    }
}
