[{
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout",
        "children": [{
                "type":"List",
                "direction": "horizontal",
                "cssClass":"header",
                "children":[{
                        "type":"List",
                        "direction": "horizontal",
                        "cssClass":"time",
                        "children":[{
                                "type": "Breadcrumb",
                                "locations": [
                                    "Initiation",
                                    "Planning",
                                    "Execution",
                                    "Closing"
                                ],
                                "variable" : "currentPhase"
                            },{
                                "type": "Button",
                                "label": ">",
                                "tooltip":"Next phase",
                                "plugins": [{
                                        "fn": "ExecuteScriptAction",
                                        "cfg": {
                                            "onClick": "//todo"
                                        }
                                    }]
                            },{
                                "type": "VariableDisplay",
                                "cssClass": "pmg-period",
                                "label": "Periods",
                                "view": "valuebox",
                                "variable": "period",
                                "dataSource": "VariableDescriptor"
    
                            }]
                    },{
                        "type":"List",
                        "direction": "horizontal",
                        "cssClass":"customers-gauges",
                        "children":[{
                                "type": "GaugeDisplay",
                                "label": "Direction",
                                "variable": "managementApproval",
                                "dataSource": "VariableDescriptor"
                            },{
                                "type": "GaugeDisplay",
                                "label": "User",
                                "variable": "userApproval",
                                "dataSource": "VariableDescriptor"
                            }]
                    },{
                        "type":"List",
                        "direction": "horizontal",
                        "cssClass":"project-gauges",
                        "children":[{
                                "type": "GaugeDisplay",
                                "label": "Fixed costs",
                                "variable": "fixedCosts",
                                "dataSource": "VariableDescriptor"
                            },{
                                "type": "GaugeDisplay",
                                "label": "Quality",
                                "variable": "quality",
                                "dataSource": "VariableDescriptor"
                            },{
                                "type": "GaugeDisplay",
                                "label": "Delay",
                                "variable": "delay",
                                "dataSource": "VariableDescriptor"
                            }]
                    },{
                        "cssClass": "pmg-timevariable",
                        "type": "VariableDisplay",
                        "label": "Available time: ",
                        "variable": "timeCards",
                        "view": "box",
                        "dataSource": "VariableDescriptor"
                    }]
            },{
                "type":"List",
                "direction": "horizontal",
                "cssClass":"horizontal-menu",
                "children":[{
                        "type": "Button",
                        "label": "Home",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Project",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 3,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Tasks",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Gantt",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Resources",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "EVM",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 7,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "LoginButton",
                        "cssClass": "menu-loginbutton",
                        "tooltip": "Logout"
                    }]
            },{
                "type":"List",
                "direction": "vertical",
                "cssClass":"main",
                "children":[{
                        "type":"List",
                        "direction": "vertical",
                        "cssClass":"vertical-menu",
                        "children":[{
                                "type": "Button",
                                "cssClass":"actions",
                                "plugins": [{
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 8,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }]
                            },{
                                "type": "Button",
                                "cssClass":"choices",
                                "plugins": [{
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 9,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }]
                            },{
                                "type": "Button",
                                "cssClass":"inbox",
                                "plugins": [{
                                        "fn": "UnreadCount",
                                        "cfg": {
                                            "variable": "inbox"
                                        }
                                    },{
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 10,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }]
                            },{
                                "type": "Button",
                                "cssClass":"history",
                                "plugins": [{
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 11,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }]
                            },{
                                "type": "Button",
                                "cssClass":"help",
                                "tooltip": "Help",
                                "plugins": [{
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 12,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }]
                            }]
                    },{
                        "label":"pageLoader",
                        "type": "PageLoader",
                        "id": "maindisplayarea",
                        "cssClass": "body",
                        "pageId": 2
                    }]
            }
        ]
    },{
        "id":2,
        "cssClass":"welcome-page",
        "type":"List",
        "children":[{
                "type": "Text",
                "content":"Welcome"
            }
        ]
    },{
        "id":8,
        "cssClass":"actions-page",
        "type":"List",
        "children":[{
                "type": "Text",
                "content":"<div class='title'><h2>Available Actions During this Phase</h2></div><div class='title'><h2>Description</h2></div>"
            },{
                "type": "MCQTabView",
                "expr": "actions.items[currentPhase.value].descriptorId"
            }
        ]
    },{
        "id":9,
        "cssClass":"choices-page",
        "type":"List",
        "children":[{
                "type": "Text",
                "content":"<div class='title'><h2>Choices to be made during this week</h2></div><div class='title'><h2>Details</h2></div>"
            },{
                "type": "MCQTabView",
                "expr": "choices.items[currentPhase.value].descriptorId"
            }
        ]
    },{
        "id":10,
        "cssClass":"inbox-page",
        "type":"List",
        "children":[{
                "type": "Text",
                "content":"<div class='title'><h2>Mailbox</h2></div><div class='title'><h2>Messages</h2></div>"
            },{
                "type": "InboxDisplay",
                "variable": "inbox"
            }
        ]
    },{
        "id":11,
        "cssClass":"history-page",
        "type":"List",
        "children":[{
                "type": "Text",
                "content":"<div class='title'><h2>Choices and Actions History</h2></div><div class='title'><h2>Details</h2></div>"
            },{
                "type": "InboxDisplay",
                "variable": "inbox"
            }
        ]
    }
]