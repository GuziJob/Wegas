[{
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout wegas-crimesim",
        "children": [{
                "label": "Indicators",
                "type": "List",
                "cssClass": "header",
                "direction": "horizontal",
                "children": [{
                        "type": "Text",
                        "content": "Crimesim",
                        "cssClass": "wegas-crimesim-title"
                    },{
                        "type": "VariableDisplay",
                        "label": "Period",
                        "view": "valuebox",
                        "variable": "period",
                        "dataSource": "VariableDescriptor"
                    },{
                        "type": "Button",
                        "label": "Next period",
                        "view": "button",
                        "tooltips": "Advance to next period",
                        "plugins": [{
                                "fn": "ExecuteScriptAction",
                                "cfg": {
                                    "onClick": "period.value += 1;\nvar qdf = lookupBean(\"QuestionDescriptorFacade\");\nqdf.setCurrentTime(evidences.descriptor, self, period.value);\nhumanResources.value = 15;\n"
                                }
                            }]
                    },{
                        "type": "VariableDisplay",
                        "label": "Costs",
                        "view": "text",
                        "variable": "cost",
                        "dataSource": "VariableDescriptor"
                    },{
                        "type": "VariableDisplay",
                        "label": "Human resources",
                        "variable": "humanResources",
                        "view": "box",
                        "dataSource": "VariableDescriptor"
                    }]
            }, {
                "id":"menu",
                "label": "Menu",
                "type": "List",
                "cssClass": "menu",
                "direction": "horizontal",
                "children": [{
                        "type": "Button",
                        "label": "Accueil",
                        "cssClass": "menu-button",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Questions",
                        "cssClass": "menu-button",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 3,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }, {
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": "questions"
                                }
                            }]
                    }, {
                        "type": "Button",
                        "label": "Pi√®ces",
                        "cssClass": "menu-button",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    }, {
                        "type": "Button",
                        "label": "Analyis results",
                        "cssClass": "menu-button",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }]
                    },{
                        "type": "Button",
                        "label": "Mails",
                        "cssClass": "menu-button",
                        "plugins": [{
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            },{
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": "inbox"
                                }
                            }]
                    },{
                        "type": "LoginButton",
                        "cssClass": "menu-loginbutton"
                    }]
            }, {
                "type": "PageLoader",
                "id": "maindisplayarea",
                "cssClass": "body",
                "pageId": 2
            }]
    },{
        "id": 2,
        "type": "Text",
        "content": "Welcome to CrimeSim."
    },{
        "id": 3,
        "type": "MCQTabView",
        "listDescriptorName": "questions"
    },{
        "id": 4,
        "type": "ScheduleDisplay"
    },{
        "id": 5,
        "type": "InboxDisplay",
        "variable": "inbox"
    }, {
        "id": 6,
        "type": "ResultsDisplay"
    }
]