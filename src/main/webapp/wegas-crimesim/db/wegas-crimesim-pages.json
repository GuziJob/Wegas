{
    "1": {
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout wegas-crimesim",
        "children": [
            {
                "label": "Indicators",
                "type": "List",
                "cssClass": "header",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Text",
                        "content": "Crimesim",
                        "cssClass": "wegas-crimesim-title"
                    }, {
                        "type": "VariableDisplay",
                        "label": "Period",
                        "view": "valuebox",
                        "variable": {
                            "name": "period"
                        }
                    }, {
                        "type": "Button",
                        "label": "Next period",
                        "view": "button",
                        "tooltip": "Advance to next period",
                        "plugins": [
                            {
                                "fn": "ExecuteScriptAction",
                                "cfg": {
                                    "onClick": "period.value += 1;\nvar qdf = lookupBean(\"QuestionDescriptorFacade\");\nqdf.setCurrentTime(evidences.descriptor, self, period.value);\nhumanResources.value = 15;\n"
                                }
                            }
                        ]
                    }, {
                        "type": "VariableDisplay",
                        "label": "Costs",
                        "view": "text",
                        "variable": {
                            "name": "cost"
                        }
                    }, {
                        "type": "VariableDisplay",
                        "label": "Human resources",
                        "variable": {
                            "name": "humanResources"
                        },
                        "view": "box"
                    }
                ]
            }, {
                "id": "menu",
                "label": "Menu",
                "type": "List",
                "cssClass": "menu",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Button",
                        "label": "Accueil",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "label": "Questions",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 3,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }, {
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": "questions"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "label": "Pièces",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "label": "Analyses results",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "label": "Mails",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }, {
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": "inbox"
                                }
                            }
                        ]
                    }, {
                        "type": "LoginButton",
                        "cssClass": "menu-loginbutton"
                    }
                ]
            }, {
                "type": "PageLoader",
                "pageLoaderId": "maindisplayarea",
                "cssClass": "body",
                "pageId": 2
            }
        ]
    },
    "2": {
        "id": 2,
        "type": "Text",
        "content": "Welcome to CrimeSim."
    },
    "3": {
        "id": 3,
        "type": "MCQTabView",
        "variable": {
            "name": "questions"
        }
    },
    "4": {
        "id": 4,
        "type": "ScheduleDisplay"
    },
    "5": {
        "id": 5,
        "type": "InboxDisplay",
        "variable": {
            "name": "inbox"
        }
    },
    "6": {
        "id": 6,
        "type": "ResultsDisplay"
    }
}
