[{
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout wegas-cep",
        "children": [{
                "type": "List",
                "cssClass": "header",
                "children": [{
                        "type": "List",
                        "direction": "horizontal",
                        "cssClass": "variables",
                        "children": [{
                                "type": "Text",
                                "content": "",
                                "cssClass": "cep-title"
                            },{
                                "type": "List",
                                "cssClass": "timeIndicators",
                                "children": [{
                                        "type": "VariableDisplay",
                                        "label": "Phase",
                                        "view": "title",
                                        "variable": {
                                            "name": "phases"
                                        }
                                    }, {
                                        "type": "VariableDisplay",
                                        "label": "Period",
                                        "view": "valuebox",
                                        "variable": {
                                            "name": "phases"
                                        }
                                    }]
                            },{
                                "type": "Button",
                                "label": "Next period",
                                "view": "button",
                                "tooltip": "Advance to next period",
