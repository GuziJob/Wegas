            "direction": "vertical",
            "children": [
                {
                    "type": "Text",
                    "cssClass": "flexitests-title",
                    "content": "FlexiTests"
                }, 
                {
                    "type": "Text",
                    "content": "_INSTRUCTIONS_"
                }, 
                {
                    "id": 2,
                    "type": "Form",
                    "plugins": [
                        {
                            "fn": "SaveObjectAction",
                            "cfg": {
                                "targetEvent": "submit",
                                "variable": {
                                    "name": "demographics"
                                }
                            }
                        }, 
                        {
                            "fn": "ExecuteScriptAction",
                            "cfg": {
                                "targetEvent": "submit",
                                "onClick": {
