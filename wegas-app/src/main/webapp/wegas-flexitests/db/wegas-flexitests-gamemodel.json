                            "cfg": {
                                "styles": {
                                    "position": "absolute",
                                    "zIndex": "10",
                                    "top": "",
                                    "left": "200px",
                                    "bottom": "30px",
                                    "right": "200px"
                                }
                            }
                        }
                    ],
                    "feedback": 1000,
                    "responseType": "link",
                    "variable": {
                        "name": "test1"
                    }
