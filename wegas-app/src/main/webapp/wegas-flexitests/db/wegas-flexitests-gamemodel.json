                    ],
                    "cfg": {
                        "fields": [
                            {
                                "name": "fullname",
                                "label": "Name",
                                "type": "string",
                                "value": ""
                            }, 
                            {
                                "name": "gender",
                                "label": "Gender",
                                "choices": [
                                    {
                                        "value": "m",
                                        "label": "M"
                                    }, 
                                    {
                                        "value": "f",
                                        "label": "F"
                                    }
                                ],
                                "type": "select",
                                "value": "m"
                            }, 
                            {
                                "fields": [
                                ],
                                "type": "variabledescriptorselect",
                                "value": "VariableDescriptorFacade.find(851)"
                            }
                        ]
                    }
                }
            ]
        },
        "1": {
            "type": "PageLoader",
            "pageLoaderId": "mainarea",
            "cssClass": "flexitests",
            "variable": {
