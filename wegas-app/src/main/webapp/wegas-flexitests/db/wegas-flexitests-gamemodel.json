{
    "@class": "GameModel",
    "name": "FlexiTests",
    "childVariableDescriptors": [{
            "@class": "NumberDescriptor",
            "name": "currentPage",
            "editorLabel": "Page",
            "label": "Page",
            "defaultInstance": {
                "@class": "NumberInstance",
                "history": [
                    2.0, 
                    2.0
                ],
                "value": 2.0
            },
            "scope": {
                "@class": "PlayerScope",
                "broadcastScope": "TeamScope"
            },
            "minValue": 2,
            "maxValue": null
        }, 
        {
            "@class": "TriggerDescriptor",
            "name": "end",
            "editorLabel": "End",
            "label": "End",
            "defaultInstance": {
                "@class": "TriggerInstance",
                "currentStateId": 1,
                "enabled": true,
                "transitionHistory": [
                ]
            },
            "scope": {
                "@class": "PlayerScope",
                "broadcastScope": "TeamScope"
            },
            "oneShot": true,
            "triggerEvent": {
                "@class": "Script",
                "content": "VariableDescriptorFacade.find(8034).size(self)>4",
                "language": "JavaScript"
            },
            "postTriggerEvent": {
                "@class": "Script",
                "content": "VariableDescriptorFacade.find(851).setValue(self, 4);",
                "language": "JavaScript"
            }
        }, 
        {
            "@class": "ObjectDescriptor",
            "name": "demographics",
            "editorLabel": "demographics",
            "label": "demographics",
            "defaultInstance": {
                "@class": "ObjectInstance",
                "properties": {
                    "": ""
                }
            },
            "scope": {
                "@class": "PlayerScope",
                "broadcastScope": "TeamScope"
            },
            "description": "",
            "properties": {
            }
        }, 
        {
            "@class": "ObjectDescriptor",
            "name": "results",
            "editorLabel": "Results",
            "label": "Results",
            "defaultInstance": {
                "@class": "ObjectInstance",
                "properties": {
                }
            },
            "scope": {
                "@class": "PlayerScope",
                "broadcastScope": "TeamScope"
            },
            "description": "",
            "properties": {
                "1": "Male",
                "2": "Female"
            }
        }
    ],
    "scriptLibrary": {
        "default": {
            "@class": "GameModelContent",
            "contentType": null,
            "content": "importPackage(org.apache.shiro);\nfunction store(object, key, value){\n    eval(object).properties.put(key, value);    \n}\nfunction getInstances(descriptors){\n    var a=[];\n    if(SecurityUtils.getSubject().hasRole(\"Registered\")){\n        for(var i in descriptors){\n            a.push(eval(descriptors[i]).getScope().getVariableInstances());\n        }\n    }else{\n        for(var i in descriptors){\n            a.push(eval(descriptors[i]).getScope().getPrivateInstances());\n        }\n    }\n    \n    return a;\n}"
        }
    },
    "cssLibrary": {
    },
    "clientScriptLibrary": {
        "load": {
            "@class": "GameModelContent",
            "contentType": null,
            "content": "Y.use(\"wegas-flexitests-controller\", function(Z){\r\n    Y.Wegas.FlexitestsController.EDITMENU= [{type: \"Button\",\r\n                label: \"Properties\",\r\n                cssClass: \"editor-exploreGameModel-button\",\r\n                plugins: [{\r\n                        fn: \"EditWidgetAction\"\r\n                    }]\r\n            }, {\r\n                type: \"Button\",\r\n                label: \"Add\",\r\n                cssClass: \"editor-exploreGameModel-button\",\r\n                plugins: [{\r\n                        \"fn\": \"WidgetMenu\",\r\n                        \"cfg\": {\r\n                            \"menuCfg\": {\r\n                                points: [\"tl\", \"tr\"]\r\n                            },\r\n                            \"event\": \"mouseenter\",\r\n                            \"children\": [{\r\n                                    type: \"Button\",\r\n                                    label: \"Single choice list\",\r\n                                    plugins: [{\r\n                                            fn: \"AddChildWidgetAction\",\r\n                                            cfg: {\r\n                                                \"childType\": \"ChoiceList\"\r\n                                            }\r\n                                        }]\r\n                                },{\r\n                                    type: \"Button\",\r\n                                    label: \"Image (fix point)\",\r\n                                    plugins: [{\r\n                                            fn: \"AddChildWidgetAction\",\r\n                                            cfg: {\r\n                                                \"childType\": \"Image\"\r\n                                            }\r\n                                        }]\r\n                                }]\r\n                        }\r\n                    }]\r\n            }];\r\n            Y.Wegas.FlexitestsController.ATTRS.plugins= Y.clone(Y.Wegas.FlexitestsController.ATTRS.plugins);\r\n            Y.Wegas.FlexitestsController.ATTRS.plugins._inputex.items.push(\r\n                        {\r\n                            type: \"Button\",\r\n                            label: \"Swap top zones\",\r\n                            data: \"SwapZone\"\r\n                        },{\r\n                            type: \"Button\",\r\n                            label: \"Expected response\",\r\n                            data: \"FlexiResponse\"\r\n                        });\r\n});"
        }
    },
    "properties": {
        "cssUri": "wegas-flexitests/css/wegas-flexitests.css"
    },
    "pages": {
        "3": {
