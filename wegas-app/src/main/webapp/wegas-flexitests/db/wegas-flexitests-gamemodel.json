                            }
                        }, 
                        {
                            "fn": "ShowAfter",
                            "cfg": {
                                "time": "1000"
                            }
                        }, 
                        {
                            "fn": "CSSPosition",
                            "cfg": {
                                "styles": {
                                    "position": "absolute",
                                    "zIndex": "10",
                                    "top": "50pt",
                                    "left": "",
                                    "bottom": "",
                                    "right": "50pt"
                                }
                            }
                        }
                    ],
                    "children": [
                        {
                            "type": "Text",
                            "content": "Female"
                        }
                    ]
                }, 
                {
                    "id": "centerElement",
                    "type": "ChoiceList",
                    "plugins": [
                        {
                            "fn": "HideAfter",
                            "cfg": {
                                "time": "0"
                            }
                        }, 
                        {
                            "fn": "ShowAfter",
                            "cfg": {
                                "time": "1500"
                            }
                        }, 
                        {
                            "fn": "CSSPosition",
                            "cfg": {
                                "styles": {
                                    "position": "absolute",
                                    "zIndex": "10",
                                    "top": "300px",
                                    "left": "300px",
                                    "bottom": "",
                                    "right": "300px"
