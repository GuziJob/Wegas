        "2": {
            "type": "List",
            "direction": "vertical",
            "children": [
                {
                    "type": "Text",
                    "cssClass": "flexitests-title",
                    "content": "FlexiTests"
                }, 
                {
                    "type": "Text",
                    "content": "_INSTRUCTIONS_"
                }, 
                {
                    "id": 2,
                    "type": "Form",
                    "cfg": {
                        "fields": [
                            {
                                "name": "fullname",
                                "label": "Name",
                                "type": "string",
                                "value": ""
                            }, 
                            {
                                "name": "gender",
                                "label": "Gender",
                                "choices": [
                                    {
                                        "value": "m",
                                        "label": "M"
                                    }, 
                                    {
                                        "value": "f",
                                        "label": "F"
                                    }
                                ],
                                "type": "select",
                                "value": "m"
                            }, 
                            {
                                "fields": [
                                ],
                                "type": "variabledescriptorselect",
                                "value": "VariableDescriptorFacade.find(851)"
                            }
                        ]
                    }
                }, 
                {
                    "disabled": false,
                    "label": "Submit",
                    "type": "Button",
                    "plugins": [
                        {
                            "fn": "ExecuteScriptAction",
                            "cfg": {
                                "targetEvent": "click",
                                "onClick": {
                                    "@class": "Script",
                                    "language": "JavaScript",
                                    "content": "currentPage.value = 3;"
                                }
                            }
                        }
                    ]
                }
            ]
        },
        "1": {
            "type": "PageLoader",
            "pageLoaderId": "mainarea",
            "cssClass": "flexitests",
            "variable": {
                "name": "currentPage"
            }
        },
        "4": {
            "type": "List",
            "direction": "vertical",
            "children": [
                {
                    "type": "Text",
                    "content": "<p>__Results__</p>"
                }
            ]
        }
    }
}