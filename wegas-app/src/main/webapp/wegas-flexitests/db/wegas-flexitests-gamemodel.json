{
    "@class": "GameModel",
    "name": "FlexiTests",
    "childVariableDescriptors": [{
            "@class": "NumberDescriptor",
            "name": "currentPage",
            "editorLabel": "Page",
            "label": "Page",
            "defaultInstance": {
                "@class": "NumberInstance",
                "history": [
                ],
                "value": 2.0
            },
            "scope": {
                "@class": "PlayerScope",
                "broadcastScope": null
            },
            "minValue": 2,
            "maxValue": null
        }, 
        {
            "@class": "TriggerDescriptor",
            "name": "end",
            "editorLabel": "end",
            "label": "end",
            "defaultInstance": {
                "@class": "TriggerInstance",
                "currentStateId": 1,
                "enabled": true,
                "transitionHistory": [
                ]
            },
            "scope": {
                "@class": "TeamScope",
                "broadcastScope": "TeamScope"
            },
            "oneShot": true,
            "triggerEvent": {
                "@class": "Script",
                "content": "VariableDescriptorFacade.find(7802).size(self)>4",
                "language": "JavaScript"
            },
            "postTriggerEvent": {
                "@class": "Script",
                "content": "VariableDescriptorFacade.find(851).setValue(self, 4);",
                "language": "JavaScript"
            }
        }, 
        {
            "@class": "QuestionDescriptor",
            "name": "test1",
            "editorLabel": "Test1",
            "label": "Test1",
            "defaultInstance": {
                "@class": "QuestionInstance",
                "replies": [
                ],
                "active": true,
                "unread": true
            },
            "scope": {
                "@class": "TeamScope"
            },
            "description": "",
            "allowMultipleReplies": true,
            "items": [
                {
                    "@class": "SingleResultChoiceDescriptor",
                    "name": "male",
                    "editorLabel": "Homme",
                    "label": "Male",
                    "defaultInstance": {
                        "@class": "ChoiceInstance",
                        "active": true,
                        "unread": true
                    },
                    "scope": {
                        "@class": "TeamScope",
                        "broadcastScope": "TeamScope"
                    },
                    "results": [
                        {
                            "@class": "Result",
                            "name": "",
                            "answer": "",
                            "files": [
                            ],
                            "impact": {
                                "@class": "Script",
                                "content": "",
                                "language": "JavaScript"
                            }
                        }
                    ],
                    "description": "<p>C'est un homme</p>",
                    "impact": null,
                    "duration": 1,
                    "cost": 0
                }, 
                {
                    "@class": "SingleResultChoiceDescriptor",
                    "name": "female",
                    "editorLabel": "Femme",
                    "label": "Female",
                    "defaultInstance": {
                        "@class": "ChoiceInstance",
                        "active": true,
                        "unread": true
                    },
                    "scope": {
                        "@class": "TeamScope",
                        "broadcastScope": "TeamScope"
                    },
                    "results": [
                        {
                            "@class": "Result",
                            "name": "",
                            "answer": "",
                            "files": [
                            ],
                            "impact": {
                                "@class": "Script",
                                "content": "",
                                "language": "JavaScript"
                            }
                        }
                    ],
                    "description": "<p>C'est une femme</p>",
                    "impact": null,
                    "duration": 1,
                    "cost": 0
                }
            ],
            "pictures": [
            ]
        }, 
        {
            "@class": "ObjectDescriptor",
            "name": "results",
            "editorLabel": "",
            "label": "Results",
            "defaultInstance": {
                "@class": "ObjectInstance",
                "properties": {
                }
            },
            "scope": {
                "@class": "TeamScope",
                "broadcastScope": "TeamScope"
            },
            "description": "",
            "properties": {
            }
        }
    ],
    "scriptLibrary": {
        "default": {
            "@class": "GameModelContent",
            "contentType": null,
            "content": "function store(object, key, value){\n    eval(object).properties.put(key, value);    \n}"
        }
    },
    "cssLibrary": {
    },
    "clientScriptLibrary": {
        "load": {
            "@class": "GameModelContent",
            "contentType": null,
            "content": "Y.use(\"wegas-flexitests-controller\", function(Z){\r\n    Y.Wegas.FlexitestsController.EDITMENU= [{type: \"Button\",\r\n                label: \"Properties\",\r\n                cssClass: \"editor-exploreGameModel-button\",\r\n                plugins: [{\r\n                        fn: \"EditWidgetAction\"\r\n                    }]\r\n            }, {\r\n                type: \"Button\",\r\n                label: \"Add\",\r\n                cssClass: \"editor-exploreGameModel-button\",\r\n                plugins: [{\r\n                        \"fn\": \"WidgetMenu\",\r\n                        \"cfg\": {\r\n                            \"menuCfg\": {\r\n                                points: [\"tl\", \"tr\"]\r\n                            },\r\n                            \"event\": \"mouseenter\",\r\n                            \"children\": [{\r\n                                    type: \"Button\",\r\n                                    label: \"ChoiceList\",\r\n                                    plugins: [{\r\n                                            fn: \"AddChildWidgetAction\",\r\n                                            cfg: {\r\n                                                \"childType\": \"ChoiceList\"\r\n                                            }\r\n                                        }]\r\n                                },{\r\n                                    type: \"Button\",\r\n                                    label: \"Image (fix point)\",\r\n                                    plugins: [{\r\n                                            fn: \"AddChildWidgetAction\",\r\n                                            cfg: {\r\n                                                \"childType\": \"Image\",\r\n                                                \"id\": \"fix-point\"\r\n                                            }\r\n                                        }]\r\n                                }]\r\n                        }\r\n                    }]\r\n            }];\r\n            Y.Wegas.FlexitestsController.ATTRS.plugins= Y.clone(Y.Wegas.FlexitestsController.ATTRS.plugins);\r\n            Y.Wegas.FlexitestsController.ATTRS.plugins._inputex.items.push(\r\n                        {\r\n                            type: \"Button\",\r\n                            label: \"Swap top zones\",\r\n                            data: \"SwapZone\"\r\n                        },{\r\n                            type: \"Button\",\r\n                            label: \"Expected response\",\r\n                            data: \"FlexiResponse\"\r\n                        });\r\n});"
        }
    },
    "properties": {
        "cssUri": "wegas-flexitests/css/wegas-flexitests.css"
    },
    "pages": {
        "3": {
            "type": "FlexitestsController",
            "cssClass": "test-page",
            "plugins": [
                {
                    "fn": "SwapZone",
                    "cfg": {
                        "after": 1
                    }
                }
            ],
            "fixPoint": 1000,
            "random": true,
            "endPage": "2",
            "store": {
                "name": "results"
            },
            "children": [
                {
                    "id": "leftElement",
                    "type": "ChoiceList",
                    "plugins": [
                        {
                            "fn": "HideAfter",
                            "cfg": {
                                "time": "0,1000"
                            }
                        }, 
                        {
                            "fn": "ShowAfter",
                            "cfg": {
                                "time": "500"
                            }
                        }, 
                        {
                            "fn": "CSSPosition",
                            "cfg": {
                                "styles": {
                                    "position": "absolute",
                                    "zIndex": "10",
                                    "top": "50pt",
                                    "left": "50pt",
                                    "bottom": "",
                                    "right": ""
                                }
                            }
                        }
                    ],
                    "children": [
                        {
                            "type": "Text",
                            "content": "Male"
                        }
                    ]
                }, 
                {
                    "id": "rightElement",
                    "type": "ChoiceList",
                    "plugins": [
                        {
