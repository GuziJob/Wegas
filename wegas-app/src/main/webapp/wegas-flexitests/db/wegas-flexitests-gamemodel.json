                                    "cfg": {
                                        "value": "2"
                                    }
                                }
                            ],
                            "url": "/jessicaPerplexe.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "1"
                                    }
                                }
                            ],
                            "url": "/lucienPortrait.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "2"
                                    }
                                }
                            ],
                            "url": "/danielaConfiant.png"
