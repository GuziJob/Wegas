                    "type": "ChoiceList",
                    "plugins": [
                        {
                            "fn": "HideAfter",
                            "cfg": {
                                "time": "0"
                            }
                        }, 
                        {
                            "fn": "ShowAfter",
                            "cfg": {
                                "time": "1500"
                            }
                        }, 
                        {
                            "fn": "CSSPosition",
                            "cfg": {
