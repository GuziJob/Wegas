                            "url": "/hectorPerplexe.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "female"
                                    }
                                }
                            ],
                            "url": "/jessicaPerplexe.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "male"
                                    }
                                }
                            ],
                            "url": "/lucienPortrait.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "female"
                                    }
                                }
                            ],
                            "url": "/danielaConfiant.png"
                        }, 
                        {
                            "type": "Image",
                            "plugins": [
                                {
                                    "fn": "FlexiResponse",
                                    "cfg": {
                                        "value": "male"
                                    }
                                }
                            ],
                            "url": "/fernandPensif.png"
                        }
                    ]
                }, 
                {
                    "id": "flexi-mcq",
