                        }, 
                        {
                            "fn": "CSSPosition",
                            "cfg": {
                                "styles": {
                                    "position": "absolute",
                                    "zIndex": "10",
                                    "top": "50pt",
                                    "left": "",
                                    "bottom": "",
                                    "right": "50pt"
                                }
