{
    "1000": {
        "type": "Form",
        "plugins": [{
                "fn": "UserPreferences"
            }]
    },
    "1": {
        "type": "PageLoader",
        "pageLoaderId": "mainarea",
        "cssClass": "flexitests",
        "variable": {
            "name": "currentPage"
        }
    },
    "2": {
        "type": "List",
        "children": [
            {
                "type": "Text",
                "content": "FlexiTests",
                "cssClass": "flexitests-title"
            }, 
            {
                "type": "Text",
                "content": "_INSTRUCTIONS_"
            }, 
            {
                "type": "Text",
                "content": "_FORMULAIRE_"
            }, 
            {
                "type": "Button",
                "label": "Submit",
                "plugins": [
                    {
                        "fn": "ExecuteScriptAction",
                        "cfg": {
                            "onClick": {
                                "@class": "Script",
                                "language": "JavaScript",
                                "content": "currentPage.value = 3;"
                            }
                        }
                    }
                ]
            }
        ]
    },
    "3": {
        "type": "FlexitestsController",
        "cssClass": "test-page",
        "fixPoint": "1000",
        "random": true,
        "children": [
            {
                "id": "leftElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "50",
                            "top": "50"
                        }
                    }, 
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0,1000",
                            "autoStart": true
                        }
                    }, 
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "500",
                            "autoStart": true
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Text",
                        "content": "Male"
                    }
                ]
            }, 
            {
                "id": "rightElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "top": "50",
                            "right": "50"
                        }
                    }, 
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0,1500",
                            "autoStart": true
                        }
                    }, 
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "1000",
                            "autoStart": true
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Text",
                        "content": "Female"
                    }
                ]
            }, 
            {
                "id": "centerElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "300",
                            "top": "300",
                            "right": "300"
                        }
                    }, 
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0",
                            "autoStart": true
                        }
                    }, 
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "1500",
                            "autoStart": true
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/hectorPerplexe.png"
                    }, 
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "female"
                                }
                            }
                        ],
                        "url": "/jessicaPerplexe.png"
                    }, 
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/lucienPortrait.png"
                    }, 
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "female"
                                }
                            }
                        ],
                        "url": "/danielaConfiant.png"
                    }, 
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/fernandPensif.png"
                    }
                ]
            }, 
            {
                "id": "flexi-mcq",
                "type": "FlexitestsMCQ",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "200",
                            "right": "200",
                            "bottom": "30"
                        }
                    }
                ],
                "feedback": 1000,
                "responseType": "link",
                "variable": {
                    "name": "test1"
                }
            }
        ],
        "plugins": [
            {
                "fn": "SwapZone"
            }
        ]
    }
}