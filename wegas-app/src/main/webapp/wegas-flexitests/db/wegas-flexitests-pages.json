{
    "1000": {
        "type": "Form",
        "plugins": [{
                "fn": "UserPreferences"
            }]
    },
    "1": {
        "type": "PageLoader",
        "pageLoaderId": "mainarea",
        "cssClass": "flexitests",
        "variable": {
            "name": "currentPage"
        }
    },
    "2": {
        "type": "List",
        "children": [{
                "type": "Text",
                "content": "FlexiTests",
                "cssClass": "flexitests-title"
            }, {
                "type": "Text",
                "content": "_INSTRUCTIONS_"
            }, {
                "id": 2,
                "type": "Form",
                "cfg": {
                    "type": "group",
                    "fields": [{
                            "name": "fullname",
                            "label": "Name"
                        }, {
                            "name": "gender",
                            "label": "Gender",
                            "type": "select",
                            "choices": [{"label": "M", "value": "m"}, {"label": "F", "value": "f"}]
                        }]
                },
                "plugins": [{
                        "fn": "SaveEntityAction",
                        "cfg": {
                            "targetEvent": "submit",
                            "variable": {
                                "name": "demographics"
                            }
                        }
                    }, {
                        "fn": "ExecuteScriptAction",
                        "cfg": {
                            "targetEvent": "submit",
                            "onClick": {
                                "@class": "Script",
                                "language": "JavaScript",
                                "content": "/*currentPage.value = 10;*/"
                            }
                        }
                    }]

            }, {
                "type": "Button",
                "label": "Submit",
                "plugins": [
                    {
                        "fn": "ExecuteScriptAction",
                        "cfg": {
                            "onClick": {
                                "@class": "Script",
                                "language": "JavaScript",
                                "content": "currentPage.value = 3;"
                            }
                        }
                    }
                ]
            }
        ]
    },
    "3": {
        "type": "FlexitestsController",
        "cssClass": "test-page",
        "fixPoint": "1000",
        "random": true,
        "children": [
            {
                "id": "leftElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "50",
                            "top": "50"
                        }
                    },
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0,1000"
                        }
                    },
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "500"
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Text",
                        "content": "Male"
                    }
                ]
            },
            {
                "id": "rightElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "top": "50",
                            "right": "50"
                        }
                    },
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0,1500"
                        }
                    },
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "1000"
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Text",
                        "content": "Female"
                    }
                ]
            },
            {
                "id": "centerElement",
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "300",
                            "top": "300",
                            "right": "300"
                        }
                    },
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0"
                        }
                    },
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "1500"
                        }
                    }
                ],
                "children": [
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/hectorPerplexe.png"
                    },
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "female"
                                }
                            }
                        ],
                        "url": "/jessicaPerplexe.png"
                    },
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/lucienPortrait.png"
                    },
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "female"
                                }
                            }
                        ],
                        "url": "/danielaConfiant.png"
                    },
                    {
                        "type": "Image",
                        "plugins": [
                            {
                                "fn": "FlexiResponse",
                                "cfg": {
                                    "value": "male"
                                }
                            }
                        ],
                        "url": "/fernandPensif.png"
                    }
                ]
            },
            {
                "id": "flexi-mcq",
                "type": "FlexitestsMCQ",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "left": "200",
                            "right": "200",
                            "bottom": "30"
                        }
                    }
                ],
                "feedback": 1000,
                "responseType": "link",
                "variable": {
                    "name": "test1"
                }
            }
        ],
        "plugins": [
            {
                "fn": "SwapZone"
            }
        ]
    }
}