{
    "1": {
        "type": "PageLoader",
        "pageLoaderId": "mainarea",
        "cssClass": "body",
        "variable": {
            "name": "currentPage"
        }
    },
    "2": {
        "type": "List",
        "children": [{
                "type": "Text",
                "content": "FlexiTests",
                "cssClass": "flexitests-title"
            }, {
                "type": "Text",
                "content": "_INSTRUCTIONS_"
            }, {
                "type": "Text",
                "content": "_FORMULAIRE_"
            }, {
                "type": "Button",
                "label": "Submit",
                "plugins": [{
                        "fn": "ExecuteScriptAction",
                        "cfg": {
                            "onClick": {
                                "@class": "Script",
                                "language": "JavaScript",
                                "content": "currentPage.value = 3;"
                            }
                        }
                    }]
            }]
    },
    "3": {
        "type": "AbsoluteLayout",
        "children": [{
                "type": "ChoiceList",
                "children": [
                    {
                        "type": "SimpleElement",
                        "content": "Male"
                    },
                    {
                        "type": "SimpleElement",
                        "contentType": "img",
                        "content": "http://stereo.gsfc.nasa.gov/beacon/latest_256/ahead_euvi_195_latest.jpg"
                    }
                ],
                "plugins": [{
                        "fn": "Position",
                        "cfg": {
                            "left": "50",
                            "top": "50"
                        }
                    },
                    {
                        "fn": "HideAfter",
                        "cfg": {
                            "time": "0,500,1010,2500"
                        }
                    },
                    {
                        "fn": "ShowAfter",
                        "cfg": {
                            "time": "505,1000,2000,3000"
                        }
                    }]
            }, {
                "type": "ChoiceList",
                "plugins": [
                    {
                        "fn": "Position",
                        "cfg": {
                            "right": "50",
                            "top": "50"
                        }
                    }
                ],
                "children": [
                    {
                        "type": "SimpleElement",
                        "content": "Female"
                    },
                    {
                        "type": "SimpleElement",
                        "content": "Female2"
                    }
                ]
            }]
    },
    "1000": {
        "type": "Form",
        "plugins": [{
                "fn": "UserPreferences"
            }]
    }
}
